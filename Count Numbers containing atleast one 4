
// 0-9 number of 4 = 1
// Similarly 10 - 19, etc except 10 - 49 . There are therefore total 9*1 + 10 numbers containing atleast one 4
// so if number of numbers till last digit count = K_N-1
// then K_N = (x>4)?((K_n-1)*x) : ((K_n-1) * (x-1) + pow(10, n))
// where n is the digit number and x is the value at that digit

#include <iostream>
#include <math.h>
using namespace std;

int main() {
	int k_n_1 = 0; // value from the last set of digits
	int num = 39;
	int n = 0;
	while(num)
	{
		int x = num%10;
		num = num/10;
		if(x < 4)
		{
			k_n_1 = (x+1)*(k_n_1);
		}
		else
		{
			k_n_1 = (x)*(k_n_1) + pow(10, n);
		}
		n++;
	}
	cout<<k_n_1;
	
	return 0;
}
